{
  "name": "orange",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint-fix": "eslint . --ext=.js,.jsx,.ts,.tsx --fix",
    "prettify": "prettier \"**/*.{js,jsx,tsx,ts,json}\" --write",
    "type-check": "tsc --noEmit",
    "test": "jest --verbose --coverage --watch false",
    "test:unit": "jest --verbose --coverage --watch false --testRegex '.*__tests__\\/.*\\.test\\.unit\\.(ts|tsx|js|jsx)$'",
    "test:int": "jest --verbose --coverage --watch false --testRegex '.*__tests__\\/.*\\.test\\.int\\.(ts|tsx|js|jsx)$'",
    "test:ci": "CI=true yarn jest",
    "test:update": "jest -u",
    "test:coverage": "jest --verbose --coverage && open ./coverage/lcov-report/index.html"
  },
  "dependencies": {
    "@react-native-community/masked-view": "^0.1.10",
    "@react-navigation/bottom-tabs": "^5.8.0",
    "@react-navigation/native": "^5.7.3",
    "@react-navigation/stack": "^5.9.0",
    "@sentry/react-native": "^1.9.0",
    "@types/react-native-vector-icons": "^6.4.6",
    "axios": "^0.20.0",
    "formik": "^2.1.5",
    "lodash": "^4.17.20",
    "react": "16.13.1",
    "react-native": "0.63.3",
    "react-native-config": "^1.3.3",
    "react-native-gesture-handler": "^1.8.0",
    "react-native-keyboard-aware-scroll-view": "^0.9.3",
    "react-native-paper": "^4.2.0",
    "react-native-reanimated": "^1.13.0",
    "react-native-safe-area-context": "^3.1.7",
    "react-native-screens": "^2.10.1",
    "react-native-sensitive-info": "^5.5.8",
    "react-native-vector-icons": "^7.1.0",
    "react-native-webview": "^10.8.3",
    "styled-components": "^5.0.1",
    "yup": "^0.29.3"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/runtime": "^7.8.4",
    "@react-native-community/eslint-config": "^2.0.0",
    "@sentry/cli": "^1.58.0",
    "@testing-library/jest-native": "^3.1.0",
    "@testing-library/react-hooks": "^3.4.1",
    "@testing-library/react-native": "^5.0.3",
    "@types/jest": "^25.2.3",
    "@types/react-native": "^0.63.3",
    "@types/react-test-renderer": "^16.9.2",
    "@types/styled-components": "^5.0.1",
    "@types/yup": "^0.29.6",
    "@typescript-eslint/eslint-plugin": "^4.5.0",
    "@typescript-eslint/parser": "^4.5.0",
    "babel-jest": "^26.6.0",
    "eslint": "^6.5.1",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react-hooks": "^4.1.2",
    "eslint-plugin-react-native": "^3.10.0",
    "eslint-plugin-react-native-a11y": "^1.3.1",
    "eslint-plugin-testing-library": "^3.9.2",
    "husky": "^4.3.0",
    "jest": "^26.6.0",
    "jest-circus": "^26.6.0",
    "jest-fetch-mock": "^3.0.3",
    "lint-staged": "^10.4.2",
    "metro-react-native-babel-preset": "^0.63.0",
    "react-native-flipper": "^0.62.0",
    "react-test-renderer": "^16.14.0",
    "stylelint": "^13.7.2",
    "stylelint-config-react-native-styled-components": "^0.3.0",
    "stylelint-processor-styled-components": "^1.10.0",
    "stylelint-react-native": "^2.2.0",
    "typescript": "^3.8.3"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged",
      "pre-push": "yarn test"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "yarn lint-fix",
      "yarn prettify"
    ],
    "*.{json}": [
      "prettier --write"
    ]
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
